{
  "name": "ats",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^6.0.1",
    "@angular/common": "^6.0.1",
    "@angular/compiler": "^6.0.1",
    "@angular/core": "^6.0.1",
    "@angular/forms": "^6.0.1",
    "@angular/http": "^6.0.1",
    "@angular/platform-browser": "^6.0.1",
    "@angular/platform-browser-dynamic": "^6.0.1",
    "@angular/router": "^6.0.1",
    "@angular/service-worker": "^6.0.1",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@types/jquery": "^3.3.22",
    "@types/sweetalert": "^2.0.4",
    "angular-6-datatable": "^0.8.0",
    "angular-highcharts": "^6.2.6",
    "bootstrap": "^4.4.1",
    "ckeditor4-angular": "^3.1.1",
    "core-js": "^2.5.6",
    "datatables.net": "^1.10.19",
    "datatables.net-bs4": "^1.10.19",
    "highcharts": "^6.2.0",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "mydatepicker": "^2.6.6",
    "ng-multiselect-dropdown": "0.2.4",
    "ngx-pagination": "^3.3.1",
    "ngx-select-dropdown": "^1.4.3",
    "ngx-spinner": "^6.1.2",
    "node-sass": "^4.14.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.6.0",
    "sweetalert2": "8.14.0",
    "xlsx": "0.12.13",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/language-service": "^6.0.1",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^2.1.0",
    "@types/highcharts": "^5.0.34",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.13",
    "codelyzer": "^4.3.0",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "puppeteer": "^1.4.0",
    "stylelint": "~9.2.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.1.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
